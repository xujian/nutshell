{
  "name": "nutshell",
  "version": "1.0.0",
  "description": "A Vue 3 UI library base on Nutui",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "sass": "dist/styles/main.scss",
  "style": "dist/styles/nutshell.scss",
  "moduleResolution": "node",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./taro": "./dist/taro/index.js",
    "./nutshell.css": "./dist/nutshell.css",
    "./nutui.css": "./dist/nutui.css",
    "./antdv.css": "./dist/antdv.css"
  },
  "scripts": {
    "patch": "node build/patch.js",
    "clean": "node build/scripts/clean.js",
    "dev": "cd ./dev && node ./scripts.buid.js dev && cd ..",
    "start": "rollup -c rollup.config.js --watch",
    "build": "yarn build:types && rollup -c rollup.config.js",
    "build:types": "tsc --pretty --emitDeclarationOnly"
  },
  "peerDependencies": {
    "@tarojs/taro": "^3.6.8",
    "vue": "^3.3.4"
  },
  "license": "MIT",
  "repository": "https://github.com/xujian/netshell.git",
  "author": "nutshell team",
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@nutui/nutui": "^4.1.2",
    "@rollup/plugin-alias": "^5.0.0",
    "postcss": "^8.4.27",
    "rollup": "^3.26.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-vue-jsx-compat": "^0.0.6",
    "typescript": "~5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1"
  },
  "vetur": {
    "tags": "dist/json/tags.json",
    "attributes": "dist/json/attributes.json"
  },
  "dependencies": {
    "@nutui/nutui-taro": "^4.1.2",
    "ant-design-vue": "4.x",
    "dayjs": "^1.11.9",
    "tippy.js": "^6.3.7",
    "vue-router": "^4.2.4",
    "vue-tippy": "v6"
  }
}
