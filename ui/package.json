{
  "name": "nutshell",
  "version": "1.0.0",
  "description": "A Vue 3 UI library base on Nutui",
  "type": "module",
  "main": "dist/nutshell.cjs.js",
  "module": "dist/nutshell.js",
  "moduleResolution": "node",
  "exports": {
    ".": {
      "import": "./dist/nutshell.js",
      "require": "./dist/nutshell.cjs.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "node build/scripts/clean.js",
    "dev": "cd ./dev && node ./scripts.buid.js dev && cd ..",
    "build": "yarn build:types && rollup -c rollup.config.js --watch",
    "build:types": "tsc --pretty --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "license": "MIT",
  "repository": "https://github.com/xujian/netshell.git",
  "author": "nutshell team",
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-vue-jsx-compat": "^0.0.6",
    "typescript": "~5.0.4"
  },
  "vetur": {
    "tags": "dist/json/tags.json",
    "attributes": "dist/json/attributes.json"
  }
}
