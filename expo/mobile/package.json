{
  "name": "expo-mobile",
  "version": "1.0.0",
  "private": true,
  "description": "Nutui expo using Nutshell library",
  "templateInfo": {
    "name": "vue3-NutUI4",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "dev": "npm run build:weapp -- --watch",
    "start": "npm run build:h5 -- --watch",
    "dev:weapp": "npm run build:weapp -- --watch",
    "build": "taro build --type weapp",
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "deploy": "rsync -avz --delete dist/h5/ root@47.107.54.47:/var/docker/expo/app/"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
