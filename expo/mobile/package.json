    "@tarojs/helper": "^4.0.6",
    "@tarojs/plugin-framework-vue3": "^4.0.6",
    "@tarojs/plugin-html": "^4.0.6",
    "@tarojs/plugin-platform-weapp": "^4.0.6",
    "@tarojs/runtime": "^4.0.6",
    "@tarojs/shared": "^4.0.6",
    "@tarojs/taro": "^4.0.6",
    "@uxda/appkit": "^4.1.24",
    "@uxda/nutshell": "^1.0.0",
    "vue": "^3.5.11"
  },
